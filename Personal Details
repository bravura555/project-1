    <script>
      $('.bootstrap-switch').on('switchChange.bootstrapSwitch', function (event, state) {
        if (state == true) {
          $(".price").html('$10.00').addClass("upgraded");
          $(".plan").html('Mini Plus').addClass("upgraded");
        } else {
          $(".price").html('$8.00').removeClass("upgraded");
          $(".plan").html('Mini').removeClass("upgraded");
        }
      });
    </script>
